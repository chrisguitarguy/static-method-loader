{
    "name": "chrisguitarguy/static-method-loader",
    "description": "A custom route loader for Symfony that calls static methods",
    "keywords": ["symfony", "routing"],
    "license": "MIT",
    "require": {
        "symfony/routing": "2.8.*",
        "symfony/config": "2.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/framework-bundle": "2.8.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Chrisguitarguy\\StaticMethodLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrisguitarguy\\StaticMethodLoader\\": "test/"
        }
    }
}

{
    "name": "chrisguitarguy/static-method-loader",
    "description": "A custom route loader for Symfony that calls static methods",
    "keywords": ["symfony", "routing"],
    "license": "MIT",
    "require": {
        "symfony/routing": "2.8.*@dev",
        "symfony/config": "2.8.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Chrisguitarguy\\StaticMethodLoader\\": "src/"
        }
    }
}
